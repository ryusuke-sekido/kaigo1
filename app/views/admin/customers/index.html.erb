<h1>利用者一覧</h1>
<% @customers.each do |customer| %>
 <p>利用者名:
 <%= link_to customer.last_name, new_admin_record_path %>
 <%= link_to customer.first_name, new_admin_record_path %>
 <%= customer.day %>曜日
 <%= link_to "削除", admin_customer_path(customer.id),
 method: :delete, "data-confirm" => "本当に削除しますか？" %>
</p>
 <% end %>
